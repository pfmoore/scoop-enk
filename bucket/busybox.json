    "post_install": [
        "if(!(test-path ~/.profile)) {",
        "    'creating ~/.profile'",
        "@\"",
        "if [ -e ~/.bashrc ]",
        "then",
        "   source ~/.bashrc",
        "fi",
        "\"@ | out-file -en oem ~/.profile",
